<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class catalog="Hibernate"
           name="POJOs.SinhVienAccount" table="SinhVienAccount">
        <id name="maSo" type="string">
            <column length="4" name="maSo"/>
            <generator class="assigned"/>
        </id>
        <property name="userName" type="string">
            <column length="10" name="username"/>
        </property>
        <property name="passWord" type="string">
            <column length="10" name="pass"/>
        </property>
        <property name="hoTen" type="string">
            <column length="10" name="hoTen"/>
        </property>
        <property name="maSinhVien" type="string">
            <column length="4" name="maSinhVien"/>
        </property>
        <property name="maLop" type="string">
            <column length="4" name="maLop"/>
        </property>
    </class>
</hibernate-mapping>